---
import BaseHead from '../components/BaseHead.astro';

interface Props {
    title?: string;
}

const { title } = Astro.props;
---

<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <BaseHead title={title} />
    </head>
</html>